<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
        <!-- 命名空间 -->
    <typeAliases>
         <typeAlias alias="Interval" type="cn.sfw.zju.system.vo.Interval"/> 
         <typeAlias alias="Interval_D" type="cn.sfw.zju.system.vo.Interval_D"/> 
         <typeAlias alias="New_Interval_D" type="cn.sfw.zju.system.vo.New_Interval_D"/> 
         <typeAlias alias="Interval_H" type="cn.sfw.zju.system.vo.Interval_H"/> 
    </typeAliases>
 
    <!-- 映射map -->
    <mappers>
    	<mapper resource="cn/sfw/zju/system/vo/sqlmap/Interval.xml"/>
    	<mapper resource="cn/sfw/zju/system/vo/sqlmap/Interval_D.xml"/>
    	<mapper resource="cn/sfw/zju/system/vo/sqlmap/New_Interval_D.xml"/>
    	<mapper resource="cn/sfw/zju/system/vo/sqlmap/Interval_H.xml"/>
    </mappers>
</configuration>